{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "mondodev2",
          "appId": "1:888572217070:android:1c3179556fd7c7118955ff",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "mondodev2",
          "configurations": {
            "android": "1:888572217070:android:1c3179556fd7c7118955ff",
            "ios": "1:888572217070:ios:944db68629c8408c8955ff",
            "web": "1:888572217070:web:2e8674407ca9e7e98955ff"
          }
        }
      }
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  },
  "hosting": {
    "site": "mondodev2",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}
